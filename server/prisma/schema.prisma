generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transactions {
  id String @id @default(uuid())
  uer_id String 
  title String
  amount Decimal @db.Decimal(10,2)
  category String
  created_at DateTime @default(now())
}